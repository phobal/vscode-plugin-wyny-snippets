{
  "CA3F": {
    "prefix": "CA3F",
    "description": "创建 antd3 版本的 Form 表单",
    "body": [
      "import React from \"react\";",
      "import { Form, Input } from \"antd\";",
      "import { WrappedFormUtils } from \"antd/lib/form/Form\";\n",
      "type $1Props = {",
      "\tform: WrappedFormUtils;",
      "};\n",
      "const formItemLayout = {",
      "\tlabelCol: { span: 4 },",
      "\twrapperCol: { span: 20 },",
      "};\n",
      "function $1(props: $1Props): JSX.Element {",
      "\tconst { form } = props;",
      "\tconst { getFieldDecorator } = form;",
      "\treturn (",
      "\t\t<Form {...formItemLayout}>",
      "\t\t\t<Form.Item label=\"姓名\">",
      "\t\t\t\t{getFieldDecorator(\"name\", {",
      "\t\t\t\t\tinitialValue: \"\",",
      "\t\t\t\t\trules: [",
      "\t\t\t\t\t\t{",
      "\t\t\t\t\t\t\trequired: true,",
      "\t\t\t\t\t\t},",
      "\t\t\t\t\t],",
      "\t\t\t\t})(<Input />)}",
      "\t\t\t</Form.Item>",
      "\t\t</Form>",
      "\t);",
      "}\n",
      "export default Form.create<$1Props>()($1);"
    ]
  }
}
